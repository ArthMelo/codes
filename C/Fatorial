#include<stdio.h>
int fatorial(int);
int main(){
    
    int numero;
    scanf("%d", &numero);
   
    printf("%d", fatorial(numero));
    return 0;

}
int fatorial(int numero){
    int resultado;
    if(numero == 0){
        return 1;
    }
    else {
        return numero * fatorial(numero - 1);
    }

}
